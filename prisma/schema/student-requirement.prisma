//student requirement model
model StudentRequirement {
  id              String                  @id @default(auto()) @map("_id") @db.ObjectId
  studentId       String?                 
  coId            String?                 @db.ObjectId
  requirementId   String?                 @db.ObjectId
  status          String                  @default("Incomplete")
  signedBy        String?
  
  clearingOfficer        ClearingOfficer?        @relation(fields: [coId], references: [id])
  officerRequirement     Requirement?            @relation(fields: [requirementId], references: [id])
}
