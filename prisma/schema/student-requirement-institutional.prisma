//student requirement model
model StudentRequirementInstitutional {
  id              String                  @id @default(auto()) @map("_id") @db.ObjectId
  studentId       String?                 
  coId            String?                 @db.ObjectId
  requirementId   String?                 @db.ObjectId
  status          String                  @default("Incomplete")
  signedBy        String?
  
  clearingOfficer ClearingOfficer?        @relation(fields: [coId], references: [id])
  // student         Student?                @relation(fields: [studentId], references: [id])
  institutionalRequirement     InstitutionalRequirement?            @relation(fields: [requirementId], references: [id])

  @@map("institutionalSigned")
}
